<?xml version="1.0" encoding="UTF-8"?>
<configs>
    <config>
        <trigger>
            <object>meetingRoomPreRecord</object>
            <operate>add</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>before</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.meeting.SaveMettingTrigger</class>
        </trigger>
        <trigger>
            <object>meetingRoomPreRecord</object>
            <operate>update</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>before</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.meeting.UpdateMeetingTrigger</class>
        </trigger>
        <trigger>
            <object>paymentRecord</object>
            <operate>update</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>after</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.pur.PaymentTrigger</class>
        </trigger>
        <trigger>
            <object>invoiceRecord</object>
            <operate>update</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>after</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.pur.InvoiceTrigger</class>
        </trigger>
        <!--媒体保留 新增前校验、新增后生成保留点位；保存前校验、保存后生成保留点位；-->
        <trigger>
            <object>mediaKeeping</object>
            <operate>add</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>before</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.mediaKeeping.SaveMediaKeeping</class>
        </trigger>
        <trigger>
            <object>mediaKeeping</object>
            <operate>add</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>after</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.mediaKeeping.SaveMediaKeepingAfter</class>
        </trigger>
        <trigger>
            <object>mediaKeeping</object>
            <operate>update</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>before</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.mediaKeeping.UpdateMediaKeeping</class>
        </trigger>
        <trigger>
            <object>mediaKeeping</object>
            <operate>update</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>after</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.mediaKeeping.UpdateMediaKeepingAfter</class>
        </trigger>

        <!--销售合同保存：开始、结束日期校验，不知道processor -->


        <!--媒体上画保存： -->

        <trigger>
            <object>contractMediaPaint</object>
            <operate>add</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>before</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.contract.UpdateContractPaintTrigger</class>
        </trigger>
        <trigger>
            <object>contractMediaPaint</object>
            <operate>update</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>before</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.contract.UpdateContractPaintTrigger</class>
        </trigger>

        <!--合同生效自动生成上画记录-->
        <trigger>
            <object>contract</object>
            <operate>update</operate>
            <processor>customEntitySaveProcessor</processor>
            <position>after</position>
            <order>1</order>
            <class>other.aakepi.bdfjfaackcpic.trigger.contract.ContractTrigger</class>
        </trigger>
    </config>
</configs>

